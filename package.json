{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "npm run kill; nodemon",
    "build": "webpack --mode=production --node-env=production",
    "kill": "kill -9 $(lsof -t -i:3070) 2>/dev/null",
    "lint": "npx eslint .",
    "fix": "npx eslint . --fix",
    "test": "npm run kill; jest --forceExit --detectOpenHandles  --watchAll --maxWorkers=1",
    "serve": "npm run kill; npm run build && node dist/api.bundle.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.30",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/pg": "^8.10.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@webpack-cli/generators": "^3.0.2",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.0.4",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.22",
    "pg": "^8.09.0"
  }
}
